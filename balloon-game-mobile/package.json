{
  "name": "rh-keynote-demo",
  "version": "0.0.0",
  "license": "MIT",
  "angular-cli": {},
  "scripts": {
    "start": "ng server",
    "postinstall": "typings install",
    "lint": "tslint \"src/**/*.ts\"",
    "format": "clang-format -i -style=file --glob=src/**/*.ts",
    "pree2e": "webdriver-manager update",
    "e2e": "protractor",
    "build": "ng build -prod && npm run scss && npm run minify:html",
    "scss": "node-sass --output-style compressed -o dist/app/ src/app/",
    "minify:js": "uglifyjs dist/vendor/blueimp-load-image/js/load-image.all.min.js dist/vendor/es6-shim/es6-shim.js dist/vendor/reflect-metadata/Reflect.js dist/vendor/systemjs/dist/system.src.js dist/vendor/zone.js/dist/zone.js -c -o dist/vendor/vendor.min.js",
    "minify:html": "html-minifier --collapse-whitespace --minify-js --minify-css -o dist/index.html src/index.html",
    "html:process": "htmlprocessor src/index.html -o .tmp/index.html"
  },
  "private": true,
  "dependencies": {
    "@angular/common": "2.0.0-rc.2",
    "@angular/compiler": "2.0.0-rc.2",
    "@angular/core": "2.0.0-rc.2",
    "@angular/http": "2.0.0-rc.2",
    "@angular/platform-browser": "2.0.0-rc.2",
    "@angular/platform-browser-dynamic": "2.0.0-rc.2",
    "@angular/router": "2.0.0-rc.2",
    "blueimp-load-image": "^2.6.1",
    "es6-shim": "^0.35.0",
    "material-design-lite": "^1.1.3",
    "phaser": "2.4.8",
    "reflect-metadata": "0.1.3",
    "rxjs": "5.0.0-beta.6",
    "systemjs": "0.19.26",
    "zone.js": "^0.6.12"
  },
  "devDependencies": {
    "angular-cli": "0.0.*",
    "clang-format": "^1.0.35",
    "codelyzer": "0.0.14",
    "ember-cli-inject-live-reload": "^1.4.0",
    "html-minifier": "^2.1.4",
    "htmlprocessor": "^0.2.3",
    "jasmine-core": "^2.4.1",
    "jasmine-spec-reporter": "^2.4.0",
    "karma": "^0.13.15",
    "karma-chrome-launcher": "^0.2.3",
    "karma-jasmine": "^0.3.8",
    "node-sass": "^3.7.0",
    "protractor": "^7.0.0",
    "ts-node": "^0.5.5",
    "tslint": "^3.6.0",
    "typescript": "^1.8.10",
    "typings": "^0.8.1",
    "uglify-js": "^2.6.2"
  }
}
